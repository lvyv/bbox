{
    "name": "bbox",
    "displayName": "bbox",
    "description": "Embedded programming extension at vehicle terminal.",
    "icon": "bbox.jpg",
    "version": "0.0.2",
    "publisher": "awen",
    "repository": {
		"type": "git",
		"url": "https://github.com/lvyv/bbox.git"
	},
    "engines": {
        "vscode": "^1.21.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.sayHello",
            "title": "Hello World"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}